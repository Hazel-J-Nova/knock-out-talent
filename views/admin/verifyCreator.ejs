<% layout('layouts/boilerplate')%>

<container>
        <% for( let creator of creators) { %>

    <form action="/api/verifyCreator/<%= creator._id %> ">
     <div class="col-md-3">
 <h2><%= creator.name %> </h2>
<% for( let [index,object]in creator.url.entries() ) { %>
    <div class="card" >
  <img src="<%= object %> " class="card-img-top" alt="a picture of <%= req.user.username %>'s <%= descriptorArray[index] %>  ">
    <div class="card-body">
   <label for="<%= descriptorArray[index] %>">
<input type="checkbox" id="<%= descriptorArray[index] %> " name="<%= descriptorArray[index] %>" value="verify">
     <%= descriptorArray[index] %>
    </label>
</div>
<% } %>
      
<button class="btn" type="submit">Verify </button>
</form>
      <% } %> 



</container>